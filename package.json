{
  "name": "zod-ome-ngff",
  "description": "zod schemas for OME-NGFF",
  "version": "0.2.0",
  "type": "module",
  "author": "Trevor Manz",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check": "tsc && publint",
    "build": "tsc && publint",
    "fmt": "deno fmt src test"
  },
  "exports": {
    "./0.1": {
      "types": "./dist/0.1.d.ts",
      "import": "./dist/0.1.js"
    },
    "./0.2": {
      "types": "./dist/0.2.d.ts",
      "import": "./dist/0.2.js"
    },
    "./0.3": {
      "types": "./dist/0.3.d.ts",
      "import": "./dist/0.3.js"
    },
    "./0.4": {
      "types": "./dist/0.4.d.ts",
      "import": "./dist/0.4.js"
    },
    ".": {
      "types": "./dist/latest.d.ts",
      "import": "./dist/latest.js"
    }
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "@changesets/cli": "^2.26.2",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "@types/node": "^20.4.2",
    "@vitest/coverage-c8": "^0.33.0",
    "camelcase": "^7.0.1",
    "glob": "^10.3.3",
    "json-schema-to-zod": "^1.1.0",
    "publint": "^0.1.16",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "packageManager": "pnpm@8.6.6",
  "engines": {
    "pnpm": "^8.0.0"
  }
}
