{
  "name": "ome-ngff-schema-zod",
  "description": "zod schemas for OME-NGFF",
  "version": "0.0.0",
  "type": "module",
  "author": "Trevor Manz",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "generate-schemas": "node ./scripts/generate-schemas.mjs",
    "test": "vitest run",
    "check": "tsc && publint",
    "build": "tsc && publint"
  },
  "exports": {
    "./0.1": {
      "types": "./dist/0.1.d.ts",
      "import": "./dist/0.1.js"
    },
    "./0.2": {
      "types": "./dist/0.2.d.ts",
      "import": "./dist/0.2.js"
    },
    "./0.3": {
      "types": "./dist/0.3.d.ts",
      "import": "./dist/0.3.js"
    },
    "./0.4": {
      "types": "./dist/0.4.d.ts",
      "import": "./dist/0.4.js"
    },
    ".": {
      "types": "./dist/latest.d.ts",
      "import": "./dist/latest.js"
    }
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "@changesets/cli": "^2.26.0",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "@types/node": "^18.14.6",
    "camelcase": "^7.0.1",
    "glob": "^9.2.1",
    "json-schema-to-zod": "^0.6.3",
    "publint": "^0.1.10",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2",
    "zod": "^3.21.0"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "packageManager": "pnpm@7.28.0",
  "engines": {
    "pnpm": "^7.0.0"
  }
}
